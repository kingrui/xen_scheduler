#!/bin/bash

source ../common/functions.sh


    rm /tmp/run.log
    echo "ft lu ep bt | cg is sp mg" >> /tmp/run.log
    
        mpirun -np 1 -bind-to-socket -slot-list 0:0-3 "/home/user189/NPB3.3/NPB3.3-MPI/bin/ft.B.1" > "/home/user189/NPB3.3/NPB3.3-MPI/output/mpi-ft-B-1-bind.txt" &
    
        mpirun -np 1 -bind-to-socket -slot-list 0:0-3 "/home/user189/NPB3.3/NPB3.3-MPI/bin/lu.B.1" > "/home/user189/NPB3.3/NPB3.3-MPI/output/mpi-lu-B-1-bind.txt" &
    
        mpirun -np 1 -bind-to-socket -slot-list 0:0-3 "/home/user189/NPB3.3/NPB3.3-MPI/bin/ep.B.1" > "/home/user189/NPB3.3/NPB3.3-MPI/output/mpi-ep-B-1-bind.txt" &
    
        mpirun -np 1 -bind-to-socket -slot-list 0:0-3 "/home/user189/NPB3.3/NPB3.3-MPI/bin/bt.B.1" > "/home/user189/NPB3.3/NPB3.3-MPI/output/mpi-bt-B-1-bind.txt" &
    
    
        mpirun -np 1 -bind-to-socket -slot-list 1:0-3 "/home/user189/NPB3.3/NPB3.3-MPI/bin/cg.B.1" > "/home/user189/NPB3.3/NPB3.3-MPI/output/mpi-cg-B-1-bind.txt" &
    
        mpirun -np 1 -bind-to-socket -slot-list 1:0-3 "/home/user189/NPB3.3/NPB3.3-MPI/bin/is.B.1" > "/home/user189/NPB3.3/NPB3.3-MPI/output/mpi-is-B-1-bind.txt" &
    
        mpirun -np 1 -bind-to-socket -slot-list 1:0-3 "/home/user189/NPB3.3/NPB3.3-MPI/bin/sp.B.1" > "/home/user189/NPB3.3/NPB3.3-MPI/output/mpi-sp-B-1-bind.txt" &
    
        mpirun -np 1 -bind-to-socket -slot-list 1:0-3 "/home/user189/NPB3.3/NPB3.3-MPI/bin/mg.B.1" > "/home/user189/NPB3.3/NPB3.3-MPI/output/mpi-mg-B-1-bind.txt" &
    
	wait
	res="ft,lu,ep,bt-cg,is,sp,mg.tar.bz2"
	pushd "/home/user189/NPB3.3/NPB3.3-MPI/output"
	tar -cjf "$res" *.txt
	rm *.txt
	popd
	cp "/home/user189/NPB3.3/NPB3.3-MPI/output/$res" output

    rm /tmp/run.log
    echo "ft cg sp bt | lu is ep mg" >> /tmp/run.log
    
        mpirun -np 1 -bind-to-socket -slot-list 0:0-3 "/home/user189/NPB3.3/NPB3.3-MPI/bin/ft.B.1" > "/home/user189/NPB3.3/NPB3.3-MPI/output/mpi-ft-B-1-bind.txt" &
    
        mpirun -np 1 -bind-to-socket -slot-list 0:0-3 "/home/user189/NPB3.3/NPB3.3-MPI/bin/cg.B.1" > "/home/user189/NPB3.3/NPB3.3-MPI/output/mpi-cg-B-1-bind.txt" &
    
        mpirun -np 1 -bind-to-socket -slot-list 0:0-3 "/home/user189/NPB3.3/NPB3.3-MPI/bin/sp.B.1" > "/home/user189/NPB3.3/NPB3.3-MPI/output/mpi-sp-B-1-bind.txt" &
    
        mpirun -np 1 -bind-to-socket -slot-list 0:0-3 "/home/user189/NPB3.3/NPB3.3-MPI/bin/bt.B.1" > "/home/user189/NPB3.3/NPB3.3-MPI/output/mpi-bt-B-1-bind.txt" &
    
    
        mpirun -np 1 -bind-to-socket -slot-list 1:0-3 "/home/user189/NPB3.3/NPB3.3-MPI/bin/lu.B.1" > "/home/user189/NPB3.3/NPB3.3-MPI/output/mpi-lu-B-1-bind.txt" &
    
        mpirun -np 1 -bind-to-socket -slot-list 1:0-3 "/home/user189/NPB3.3/NPB3.3-MPI/bin/is.B.1" > "/home/user189/NPB3.3/NPB3.3-MPI/output/mpi-is-B-1-bind.txt" &
    
        mpirun -np 1 -bind-to-socket -slot-list 1:0-3 "/home/user189/NPB3.3/NPB3.3-MPI/bin/ep.B.1" > "/home/user189/NPB3.3/NPB3.3-MPI/output/mpi-ep-B-1-bind.txt" &
    
        mpirun -np 1 -bind-to-socket -slot-list 1:0-3 "/home/user189/NPB3.3/NPB3.3-MPI/bin/mg.B.1" > "/home/user189/NPB3.3/NPB3.3-MPI/output/mpi-mg-B-1-bind.txt" &
    
	wait
	res="ft,cg,sp,bt-lu,is,ep,mg.tar.bz2"
	pushd "/home/user189/NPB3.3/NPB3.3-MPI/output"
	tar -cjf "$res" *.txt
	rm *.txt
	popd
	cp "/home/user189/NPB3.3/NPB3.3-MPI/output/$res" output

    rm /tmp/run.log
    echo "is ep mg | lu ft cg bt sp" >> /tmp/run.log
    
        mpirun -np 1 -bind-to-socket -slot-list 0:0-3 "/home/user189/NPB3.3/NPB3.3-MPI/bin/is.B.1" > "/home/user189/NPB3.3/NPB3.3-MPI/output/mpi-is-B-1-bind.txt" &
    
